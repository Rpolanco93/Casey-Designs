# Ignore everything by default
*

# Allow only needed files
!/app
!/migrations
!/react-vite
!/Pipfile
!/Pipfile.lock
!/run.sh
!/requirements.txt

# Ignore any python generated files
**/__pycache__
**/*.pyc

# Ignore dist folder for react-vite and node modules
react-vite/dist
react-vite/node_modules

# Ignore migration files and scripts
migrations/tools/bin/*
migrations/tools/scripts/models
migrations/*.sql

# Ignore any READMEs
**/README*

# Ignore OSX files
**/.DS_Store

# Ignore .gitignore
**/.gitignore
